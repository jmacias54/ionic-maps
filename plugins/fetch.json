{
    "cordova-plugin-geolocation": {
        "source": {
            "type": "registry",
            "id": "cordova-plugin-geolocation@3.0.0"
        },
        "is_top_level": true,
        "variables": {
            "GEOLOCATION_USAGE_DESCRIPTION": "To locate you"
        }
    },
    "cordova-plugin-googlemaps": {
        "source": {
            "type": "registry",
            "id": "cordova-plugin-googlemaps"
        },
        "is_top_level": true,
        "variables": {
            "API_KEY_FOR_ANDROID": "YOUR_ANDROID_API_KEY_IS_HERE",
            "API_KEY_FOR_IOS": "YOUR_IOS_API_KEY_IS_HERE"
        }
    }
}